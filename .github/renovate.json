{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>scop/common"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "custom_components/jatekukko/manifest.json"
      ],
      "matchStrings": [
        "(?<depName>[\\w-]+)(?<currentValue>==[a-z0-9.]+)"
      ],
      "datasourceTemplate": "pypi"
    }
  ]
}
